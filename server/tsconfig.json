{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "lib": ["ES2020"],
    "module": "CommonJS",
    "allowJs": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "outDir": "dist",
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2020"
  },
  "exclude": ["node_modules", "generated", "dist"]
}
